@*CABEÇALHO*@
<ol class="breadcrumb">

    <li>@Html.ActionLink("Home", "Home", "Home", null, null)</li>
    <li>Consultas</li>
    <li>@Html.ActionLink("Estoque Vendas", "EstoqueVendas", "Consultas", null, null)</li>
</ol>

<div class="panel panel-default">
